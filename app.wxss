@import 'style/weui.wxss';

/*** 定义字体为微软雅黑、苹方 ***/

view {
  font-family: Helvetica, Tahoma, "Microsoft YaHei", "PingFang SC", sans-serif;
}

/*** 字体图标库(ttp://www.iconfont.cn/plus) ***/

@font-face {
  font-family: 'iconfont';
  src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');font-weight: 500;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
}

.icon-user:before { content: "\e600"; }

.icon-arrow:before { content: "\e60f"; }

.icon-notice:before { content: "\e628"; }

.icon-weep:before { content: "\e648"; }

.icon-zhifubao:before { content: "\e7da"; }

.icon-wxpay:before { content: "\e617"; }

.icon-verify:before { content: "\e68e"; }

.icon-bank:before { content: "\e65b"; }

.icon-apply:before { content: "\e661"; }

.icon-calendar:before { content: "\e610"; }

.icon-warning:before { content: "\e611"; }

/*** 弹性布局 ***/

.flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.flex-x {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
}

.flex-y {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.flex-xy {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.flex-column {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}

.flex-grow {
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
  flex-grow: 1;
}

page {
  background: #f1f1f4;
  overflow: auto;
}

scroll-view {
  position: fixed;
  width: 100%;
  height: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

textarea::-webkit-input-placeholder {
  position: relative;
}
